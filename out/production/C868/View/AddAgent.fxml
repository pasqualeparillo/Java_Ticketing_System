<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="240.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddAgent">
    <children>
        <Label layoutX="29.0" layoutY="96.0" text="Username:">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Label>
        <TextField fx:id="agentUsername" layoutX="29.0" layoutY="114.0" prefWidth="282.0" style="-fx-background-color: #E5E5E5;" />
        <Label layoutX="29.0" layoutY="146.0" text="Password:">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <PasswordField fx:id="agentPassword" accessibleRole="PASSWORD_FIELD" layoutX="29.0" layoutY="164.0" prefWidth="282.0" style="-fx-background-color: #E5E5E5;" />
        <Button layoutX="226.0" layoutY="200.0" mnemonicParsing="false" onAction="#addAgent" style="-fx-background-color: #9147FF;" text="Save" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
        <Button layoutX="273.0" layoutY="200.0" mnemonicParsing="false" onAction="#exitToMain" style="-fx-background-color: #EB0400;" text="Exit" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
        <Label layoutX="121.0" layoutY="14.0" text="Add Agent:">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Label layoutX="29.0" layoutY="42.0" text="Email:">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <TextField fx:id="agentEmail" layoutX="29.0" layoutY="60.0" prefHeight="25.0" prefWidth="282.0" style="-fx-background-color: #E5E5E5;" />
    </children>
</AnchorPane>
