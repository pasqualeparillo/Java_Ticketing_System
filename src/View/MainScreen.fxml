               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="644.0" layoutY="220.0" mnemonicParsing="false" onAction="#modifyAgent" style="-fx-background-color: #EFEFF1;" text="Modify">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="596.0" layoutY="220.0" mnemonicParsing="false" onAction="#addAgent" style="-fx-background-color: #EFEFF1;" text="Add">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <TextField fx:id="agentSearch" layoutX="610.0" layoutY="21.0" onKeyTyped="#searchForAgent" />
